<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	 http://www.springframework.org/schema/beans/spring-beans.xsd
	 http://www.springframework.org/schema/context 
	 http://www.springframework.org/schema/context/spring-context-4.3.xsd"
	default-init-method="customInit" default-destroy-method="customDestroy">

	<bean name="activityBean" class="com.revature.model.bean.Activity" />
	<bean name="credentialBean" class="com.revature.model.bean.Credential" />
	<bean name="itemBean" class="com.revature.model.bean.Item" />
	<bean name="playerBean" class="com.revature.model.bean.Player" />
	<bean name="playerItemBean" class="com.revature.model.bean.PlayerItem" />

</beans>